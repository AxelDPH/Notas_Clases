package loops;

public class WhileLoop {
   public static void main (String[] args) {
	   /*
	    * While. El ciclo While ejecuta continuamente un bloque de codigo hasta que se cumpla
	    * While (expresion){
	    //bloque de codigo a ejecutar
	     * (contador - iterador)
	     * }
	    * */
	   /**
	   int cuenta = 1;
	   while (cuenta < 11) {
	     System.out.println("La cuenta es de: " + cuenta);
	     cuenta++;
	   }
	   */
	   int cuenta = 0;
	   while (cuenta < 10) {
		   cuenta++;
	     System.out.println("La cuenta es de: " + cuenta);
	     
	   }
	   
	   
   }//fin de metodo main
}//fin class
